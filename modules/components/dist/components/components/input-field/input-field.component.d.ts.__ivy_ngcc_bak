import { OnInit, EventEmitter, ElementRef } from '@angular/core';
export declare class InputFieldComponent implements OnInit {
    text?: string;
    placeholder: string;
    textChange: EventEmitter<any>;
    enter: EventEmitter<any>;
    editable: boolean;
    disabled: boolean;
    fontSize: number;
    container: ElementRef;
    ngOnInit(): void;
    inputChanged(): void;
    enterPressed(): void;
}
